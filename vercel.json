{
  "functions": {
    "api/index.ts": {
      "includeFiles": "server/**"
    },
    "api/health.ts": {
      "includeFiles": "server/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    }
  ]
}
